$(BINDIR)\exe2.exe      : $(OBJDIR)\exe2.obj $(OBJDIR)\bios.obj
$(BINDIR)\nde.com       : $(OBJDIR)\nde.obj
$(BINDIR)\x.com         : $(OBJDIR)\x.obj

