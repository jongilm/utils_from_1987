SRCDIR=.
OBJDIR=objects
LIBDIR=\tools
DODLIB=\doddle\lib
BINDIR=bin

{$(SRCDIR)}.asm{$(OBJDIR)}.obj:
   masm -t -z $**,$@;

{$(SRCDIR)}.c{$(OBJDIR)}.obj :
   cl /nologo /AL /Ox /c /W3 /Fo$@ $**

{$(OBJDIR)}.obj{$(BINDIR)}.sys :
   link /nologo $**,$*.exe;
   exe2bin $*.exe $@
   del $*.exe

{$(OBJDIR)}.obj{$(BINDIR)}.com :
   link /nologo /tiny $**,$@;

{$(OBJDIR)}.obj{$(BINDIR)}.exe :
#   link /nologo $**,$@;
   cl /nologo /Fe$@ $**

#{$(SRCDIR)}.c{$(BINDIR)}.exe :
#   cl /nologo /AL /Ox /c /W3 /Fo$@ $**
#   link /nologo /E /CP:1 $*.obj,,,toolsl.lib commsl.lib doddle.lib;

#{$(SRCDIR)}.c{$(BINDIR)}.exe :
#   cl /nologo /AL /Ox $** c:\c600\lib\setargv.obj /link /E /CP:1 toolsl.lib commsl.lib doddle.lib

#{$(SRCDIR)}.c{$(BINDIR)}.exe :
#   cl /nologo /AL /Ox $** /link /E /CP:1 toolsl.lib commsl.lib doddle.lib

#{$(SRCDIR)}.c{$(BINDIR)}.com :
#   cl /nologo /AT /Ox $*.c /link toolss.lib

#################################################################
ALL : ASM_MSG ASM C_MSG C
   @echo $@ Done

ASM_MSG :
   @echo Building ASM's...

C_MSG :
   @echo Building C's...

PAS_MSG :
   @echo Building PAS's...

#здддддддддддддддддддддддддддддддддддддддддддддддддд©
#ЁASM                                               Ё
#юдддддддддддддддддддддддддддддддддддддддддддддддддды

ASM : $(BINDIR)\asy.com\
      $(BINDIR)\asydvr.sys\
      $(BINDIR)\balls.exe\
      $(OBJDIR)\c.obj\
      $(BINDIR)\com_2.com\
      $(BINDIR)\cursor.com\
      $(BINDIR)\drive.com\
      $(BINDIR)\example.com\
      $(BINDIR)\exe2.exe\
      $(BINDIR)\nde.com\
      $(OBJDIR)\c_module.obj\
      $(BINDIR)\x.com\
      $(BINDIR)\com.com\
      $(BINDIR)\exe.exe
   @echo $@ Done

$(BINDIR)\asy.com       : $(OBJDIR)\asy.obj
$(BINDIR)\asydvr.sys    : $(OBJDIR)\asydvr.obj
$(BINDIR)\balls.exe     : $(OBJDIR)\balls.obj
$(BINDIR)\com.com       : $(OBJDIR)\com.obj
$(BINDIR)\com_2.com     : $(OBJDIR)\com_2.obj $(OBJDIR)\bios.obj
$(BINDIR)\cursor.com    : $(OBJDIR)\cursor.obj
$(BINDIR)\drive.com     : $(OBJDIR)\drive.obj
$(BINDIR)\example.com   : $(OBJDIR)\example.obj
